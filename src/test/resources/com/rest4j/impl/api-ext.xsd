<!--

XML Scheme for extensions.
-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			targetNamespace="http://rest4j.com/api-description/ext" xmlns="http://rest4j.com/api-description/ext"
			elementFormDefault="qualified" attributeFormDefault="unqualified">

	<xsd:element name="field-ext">
		<xsd:complexType>
			<xsd:attribute name="attr" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="param-ext"/>
	<xsd:element name="endpoint-ext"/>
	<xsd:element name="model-ext"/>

</xsd:schema>